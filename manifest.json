{
  "manifest_version": 2,

  "name": "Harvey",
  "description": "This extension shows a picture of Steve Harvey.",
  "version": "1.0",

  "browser_action": {
    "default_title": "Here's STEVE!!",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}